{
  "name": "agent-market",
  "private": true,
  "version": "0.1.0",
  "description": "AgentMarket monorepo for agent-to-agent marketplace MVP",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "eval:import-genesis": "tsx scripts/import-genesis-scenarios.ts --input Genesis-Rebuild-main/Genesis-Rebuild-main/tests/rogue/scenarios/orchestration_p1.yaml --output configs/evaluations/genesis-orchestration.json --vertical orchestration --limit 50 --agentTags orchestration,planner",
    "eval:run": "tsx scripts/run-evaluations.ts",
    "service-account:create": "tsx scripts/create-service-account.ts"
  },
  "packageManager": "npm@11.6.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.3.3",
    "tsx": "^4.11.0",
    "turbo": "^2.1.0",
    "typescript": "^5.6.3",
    "yaml": "^2.8.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --max-warnings=0",
    "*.{json,md,yml,yaml}": "prettier --check"
  },
  "dependencies": {
    "stripe": "^19.3.1"
  }
}
